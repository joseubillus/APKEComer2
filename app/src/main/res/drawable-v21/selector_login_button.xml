<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="10dp"/>
            <size android:height="55dp"/>
            <stroke android:width="1dp"
                android:color="#FFEB3B"/>
            <padding android:left="20dp"/>
            <gradient android:startColor="#45BF11"
                android:centerColor="#2D800F"
                android:endColor="#102507"
                android:type="linear"
                android:angle="270"
                />
        </shape>
    </item>
    <item android:state_focused="false">
        <shape android:shape="rectangle">
            <corners android:radius="10dp"/>
            <size android:height="55dp"/>
            <stroke android:width="1dp"
                android:color="#FFEB3B"/>
            <padding android:left="20dp"/>
            <gradient android:startColor="#001AFF"
                android:centerColor="#0D0A38"
                android:endColor="#020202"
                android:type="linear"
                android:angle="270"
                />
        </shape>
    </item>
</selector>